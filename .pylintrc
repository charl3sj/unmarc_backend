[MASTER]
load-plugins=pylint_django
init-hook='import sys; sys.path.append("./unmarc")'

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,too-few-public-methods,pointless-string-statement

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
